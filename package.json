{
    "name": "docker-classes",
    "description": "Opinionated high level wrapper classes for the Docker API using dockerode",
    "version": "2.0.3",
    "np": {
        "contents": "dist"
    },
    "files": [
        "./dist"
    ],
    "main": "./dist/index.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/reedHam/node-docker-classes.git"
    },
    "license": "MIT",
    "scripts": {
        "test": "jest",
        "build": "tsc --declaration",
        "format": "eslint --fix 'lib/**/*' && prettier --write './lib/**/*.{ts,js}'",
        "prepublish": "yarn test && yarn build"
    },
    "dependencies": {
        "dockerode": "^3.3.1",
        "dotenv": "^16.0.0"
    },
    "devDependencies": {
        "@types/dockerode": "^3.3.8",
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.14.0",
        "jest": "^27.0.0",
        "prettier": "^2.6.2",
        "ts-jest": "^27.1.4",
        "typescript": "^4.6.3"
    },
    "keywords": [
        "docker",
        "dockerode",
        "docker-api",
        "docker-api-wrapper",
        "docker-api-wrapper-node",
        "class",
        "wrapper"
    ]
}
